{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": ""
  },
  "Courses": {
    "PerPage": 10,
    "Order" : {
      "By" : "Title",
      "Ascending" : true,
      "Allow": ["Title", "Rating", "CurrentPrice"]
    }
  },
  "Cache": {
    "Time": 60
  },
  "ResponseCache": {
    "Home": {
      "Duration" : 60,
      "Location" : "Client",
      "VaryByQueryKeys": ["page"]
    }
  }
}

{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": ""
  },
  "Courses": {
    "PerPage": 6,
    "InHome": 3,
    "Order" : {
      "By" : "",
      "Ascending" : false,
      "Allow": ["Id","Title", "Rating", "CurrentPrice"]
    }
  },
  "Cache": {
    "Time": 60
  },
  "ResponseCache": {
    "Home": {
      "Duration" : 60,
      "Location" : "Client",
      "VaryByQueryKeys": ["page"]
    }
  }
}
